{
    "version": "@VERSION@",
    "requires": {
	"cockpit": "122"
    },

    "tools": {
        "index": {
            "label": "Applications",
            "keywords": {
                "general": {
                    "matches": ["plugin", "apps", "addon", "add-on"]
                }
            }
        }
    },

    "content-security-policy": "img-src *",

    "config": {
        "appstream_config_packages": @with_appstream_config_packages@,
        "appstream_data_packages": @with_appstream_data_packages@
    }

}
